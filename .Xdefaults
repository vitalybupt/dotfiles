! URxvt Appearance
URxvt.geometry:             90x30
URxvt.transparent:          false
URxvt.fading:               0
URxvt.loginShell:           true
URxvt.saveLines:            50
URxvt.internalBorder:       3
URxvt.lineSpace:            0


URxvt.scrollBar: false
URxvt.iso14755: false

URxvt.allow_bold:           false

!URxvt.font: xft:terminal:style=Regular:size=14
URxvt.font: xft:Iosevka Custom:style=Regular:size=15
URxvt*boldFont: xft:Iosevka Custom:style=Bold:size=15

! Fix font space
URxvt*letterSpace: -1


!! transparence configure
URxvt.depth: 32
URxvt.background: rgba:0000/2b00/3600/f000

